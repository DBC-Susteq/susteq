.row
.col-lg-12
	%h1.page-header Water Points
.col-lg-12
	.panel.panel-default.mt20
		.panel-heading
			%i.fa.fa-map-marker.fa-fw
			Map
			.pull-right
				.minimize.map-button
					%i.fa.fa-minus.fa-fw
		.panel-body#map.pumps-index
.col-lg-12
	.panel.panel-default.mt20
		.panel-heading
			%i.fa.fa-dashboard.fa-fw
			Chart
		.panel-body
			#chart-container
.col-lg-12
	= render 'list'
